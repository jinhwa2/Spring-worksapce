<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.test.mapper.UserMemberMapper">

	<insert id="insertUserMember"
		parameterType="com.kh.test.dto.UserMember">
		INSERT INTO USERMEMBER(US_ID, US_EMAIL)
		VALUES (userMember_seq.NEXTVAL, #{us_Id}, #{us_Email})



	</insert>






</mapper>        